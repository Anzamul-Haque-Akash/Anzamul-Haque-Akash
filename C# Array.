using System;

namespace MyApplication
{
    class Program
    {
        static void Main(string[] args)
        {
            int[] arr = new int[5];

            for (int i = 0; i < 5; i++)
            {
                Console.Write($"Enter the {i+1}th element of the array : ");
                arr[i] = Convert.ToInt32(Console.ReadLine());

            }

            for (int i = 0; i < 5; i++)
            {
                Console.Write(arr[i] + " ");
            }
            Console.Write("\n");
        }
    }
}
