using System;

namespace main
{
    class MainClass
    {
        public static void Main(string[] args)
        {
            Console.Write("Anzamul Haque "); //Write() print two string in same line.
            Console.Write("Akash\n");

            Console.WriteLine("Hi, my name is Anzamul Haque Akash."); //Write() print two string in diffrent line.
            Console.Write("I am 21 years old.");
        }
    }
}
