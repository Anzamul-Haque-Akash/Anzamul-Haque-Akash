const prompt = require('prompt-sync')(); 

function ADD(num1, num2){
    return num1 + num2;
}
var tc;
tc = prompt("Enter the test case : ");

while(tc--){
    var num1, num2;
    num1 = prompt("Enter the numner 1 : ");
    num2 = prompt("Enter the number 2 : ");

    num1 = parseInt(num1);
    num2 = parseInt(num2);

    console.log("Sum is : " + ADD(num1, num2));
}
